{
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "bracketSameLine": false,
  "useTabs": false,
  "trailingComma": "all",
  "printWidth": 100,
  "tabWidth": 4,
  "semi": true,
  "singleQuote": true,
  "overrides": [
    {
      "files": [
        "*.yml",
        "*.json"
      ],
      "options": {
        "tabWidth": 2,
        "singleQuote": false
      }
    }
  ]
}
